package fr.test;

import java.util.Scanner;

public class Echo {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String str = sc.next();
		String ary[] = str.split(" ");
		System.out.println( ary.length);
		for (String string : ary) {
			System.out.println(string);

		}
	}

}
