package fr.test;

public class Test2 {

	static int findLargest(int[] numbers) {
		int sum = 0;
		for (int i = 0; i < numbers.length; i++) {
			int j = numbers[i];
			if (numbers[i] > 10 && numbers[i] <= 100) {
				sum += numbers[i];
			}
		}
		return sum;
	}

	public static void main(String[] args) {
		int[] arr = { 1};

		Test2 test2 = new Test2();
		System.out.println(test2.findLargest(arr));
	}

}
